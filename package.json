{
  "name": "jsskeleton",
  "version": "1.0.0",
  "description": "Javascript skeleton for universal application",
  "main": "index.js",
  "scripts": {
    "start-win-uni-dark": "webpack-dev-server --inline --hot --port 8082 --content-base src/platforms/win-uni-dark/",
    "start-win-uni-light": "webpack-dev-server --inline --hot --port 8082 --content-base src/platforms/win-uni-light/",
    "start": "webpack-dev-server --inline --hot --port 8082 --content-base src",
    "build": "webpack",
    "buildapp": "node ./node_modules/lessapp --win --output-dir build/ --bundle build/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/honzapospi/jsskeleton.git"
  },
  "author": "Jan Pospisil",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/honzapospi/jsskeleton/issues"
  },
  "homepage": "https://github.com/honzapospi/jsskeleton#readme",
  "devDependencies": {
    "babel-core": "^6.7.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "lessapp": "^1.0.1",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "redux": "^3.3.1",
    "redux-promise": "^0.5.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
